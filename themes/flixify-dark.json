{
	"name": "Flixify Dark",
	"type": "dark",
	"$schema": "vscode://schemas/color-theme",
	"colors": {
		"activityBar.background": "#21252b",
		"activityBar.foreground": "#D7DAE0",
		"activityBarBadge.background": "#528bff",
		"activityBarBadge.foreground": "#F8FAFD",
		"button.background": "#528bff",
		"dropdown.background": "#1d1f23",
		"diffEditor.insertedTextBackground": "#007e9733",
		"dropdown.border": "#181A1F",
		"editor.background": "#21252b",
		"editor.foreground": "#F8F8F2",
		"editorError.foreground": "#c24038",
		"editorRuler.foreground": "#484848",
		//"editor.lineHighlightBackground": "#383E4A",
		"editor.selectionBackground": "#3E4451",
		"editorCursor.foreground": "#f8f8f0",
		"editor.findMatchBackground": "#42557B",
		"editor.findMatchHighlightBackground": "#314365",
		"editorGroup.background": "#181A1F",
		"editorGroup.border": "#181A1F",
		"editorGroupHeader.tabsBackground": "#21252B",
		"editorIndentGuide.background": "#3B4048",
		"editorLineNumber.foreground": "#495162",
		"editorWhitespace.foreground": "#3B4048",
		"editorHoverWidget.background": "#21252B",
		"editorHoverWidget.border": "#181A1F",
		"editorSuggestWidget.background": "#21252B",
		"editorSuggestWidget.border": "#181A1F",
		"editorSuggestWidget.selectedBackground": "#2c313a",
		"input.background": "#1d1f23",
		"list.activeSelectionBackground": "#2c313a",
		"list.activeSelectionForeground": "#d7dae0",
		"list.focusBackground": "#383E4A",
		"list.hoverBackground": "#292d35",
		"list.highlightForeground": "#C5C5C5",
		"list.inactiveSelectionBackground": "#2c313a",
		"list.inactiveSelectionForeground": "#d7dae0",
		"notification.background": "#21252b",
		"scrollbarSlider.background": "#4E566680",
		"scrollbarSlider.activeBackground": "#747D9180",
		"scrollbarSlider.hoverBackground": "#5A637580",
		"sideBar.background": "#2c3038",
		"sideBarSectionHeader.background": "#21252b",
		"statusBar.background": "#2f5691",
		"statusBar.foreground": "#f8f8f2",
		"statusBarItem.hoverBackground": "#2c313a",
		"statusBar.noFolderBackground": "#21252B",
		"statusBar.debuggingBackground": "#21252B",
		"tab.activeBackground": "#21252b",
		"tab.border": "#181A1F",
		"tab.inactiveBackground": "#2c3038",
		"titleBar.activeBackground": "#282c34",
		"titleBar.activeForeground": "#9da5b4",
		"titleBar.inactiveBackground": "#282C34",
		"titleBar.inactiveForeground": "#6B717D",
		"gitDecoration.modifiedResourceForeground": "#3dbbff",
		"scrollbar.shadow": "#0c0d0f"
	},
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#8C8C8C"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#FFEE99"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#ff8cf5"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#ff8cf5"
			}
		},
		{
			"scope": "constant.character, constant.other",
			"settings": {
				"foreground": "#ff8cf5"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#fa3e83"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#fa3e83",
				"fontStyle": ""
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#66D9EF",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "entity.name.class",
			"settings": {
				"foreground": "#ace340",
				"fontStyle": "underline"
			}
		},
		{
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#ace340",
				"fontStyle": "italic underline"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#ace340",
				"fontStyle": ""
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#fcb058",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#fa3e83",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#ace340",
				"fontStyle": ""
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#66D9EF",
				"fontStyle": ""
			}
		},
		{
			"scope": "support.constant",
			"settings": {
				"foreground": "#66D9EF",
				"fontStyle": ""
			}
		},
		{
			"scope": "support.type, support.class",
			"settings": {
				"foreground": "#66D9EF",
				"fontStyle": ""
			}
		},
		{
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"scope": "support.other.namespace, entity.name.type.namespace",
			"settings": {
				"foreground": "#FFB2F9"
			}
		},
		{
			"scope": "support.other.namespace.use-as.php",
			"settings": {
				"foreground": "#66D9EF"
			}
		},
		{
			"scope": "variable.language.namespace.php",
			"settings": {
				"foreground": "#D66990"
			}
		},
		{
			"scope": "punctuation.separator.inheritance.php",
			"settings": {
				"foreground": "#fa3e83"
			}
		},
		{
			"scope": "support.function.misc.css, support.constant.property-value.css, support.constant.font-name.css",
			"settings": {
				"foreground": "#FFEE99"
			}
		},
		{
			"scope": "meta.tag.template.value.twig, meta.tag.template.block.twig",
			"settings": {
				"foreground": "#CD5AC5"
			}
		},
		{
			"scope": "keyword.control.twig",
			"settings": {
				"foreground": "#E05D8C"
			}
		},
		{
			"scope": "variable.other.twig",
			"settings": {
				"foreground": "#E5A5E0"
			}
		},
		{
			"scope": "variable.other.property.twig",
			"settings": {
				"foreground": "#FFE1FC"
			}
		},
		{
			"scope": "constant.language.twig",
			"settings": {
				"foreground": "#FFD2A6"
			}
		},
		{
			"scope": "constant.numeric.twig",
			"settings": {
				"foreground": "#FFD0FB"
			}
		},
		{
			"scope": "support.function.twig",
			"settings": {
				"foreground": "#90E7F7"
			}
		},
		{
			"scope": "meta.function-call.other.twig",
			"settings": {
				"foreground": "#FAB85A"
			}
		},
		{
			"scope": "meta.function-call.twig",
			"settings": {
				"foreground": "#FAB85A"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#F8F8F0",
				"background": "#fa3e83",
				"fontStyle": ""
			}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#F8F8F0",
				"background": "#ff8cf5"
			}
		},
		{
			"scope": "text source",
			"settings": {
				"background": "#3D3D3D55"
			}
		},
		{
			"scope": "meta.diff, meta.diff.header",
			"settings": {
				"foreground": "#E0EDDD",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#D3201F",
				"fontStyle": ""
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#BF3904",
				"fontStyle": ""
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#60CF30"
			}
		},
		{
			"scope": "meta.diff, meta.diff.range",
			"settings": {
				"foreground": "#3BC0F0"
			}
		},
		{
			"scope": "markup.heading.markdown, markup.heading.1.markdown, markup.heading.2.markdown",
			"settings": {
				"foreground": "#ace340"
			}
		},
		{
			"scope": "markup.bold.markdown",
			"settings": {
				"foreground": "#F8F8F2",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.italic.markdown",
			"settings": {
				"foreground": "#F8F8F2",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "punctuation.definition.bold.markdown, punctuation.definition.italic.markdown, punctuation.definition.raw.markdown",
			"settings": {
				"foreground": "#D3201F"
			}
		},
		{
			"scope": "markup.list.unnumbered.markdown",
			"settings": {
				"foreground": "#ff8cf5"
			}
		},
		{
			"scope": "markup.list.numbered.markdown",
			"settings": {
				"foreground": "#ff8cf5"
			}
		},
		{
			"scope": "markup.raw.block.markdown, markup.raw.inline.markdown",
			"settings": {
				"foreground": "#FFEE99"
			}
		},
		{
			"scope": "markup.quote.markdown, punctuation.definition.blockquote.markdown",
			"settings": {
				"foreground": "#6C71C4"
			}
		},
		{
			"scope": "meta.separator.markdown",
			"settings": {
				"foreground": "#ff8cf5"
			}
		},
		{
			"scope": "meta.image.inline.markdown, markup.underline.link.markdown",
			"settings": {
				"foreground": "#8C8C8C",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "string.other.link.title.markdown, string.other.link.description.markdown",
			"settings": {
				"foreground": "#ace340"
			}
		},
		{
			"scope": "punctuation.definition.link.markdown",
			"settings": {
				"foreground": "#8C8C8C"
			}
		},
		{
			"scope": "punctuation.definition.metadata.markdown",
			"settings": {
				"foreground": "#8C8C8C"
			}
		},
		{
			"scope": "punctuation.definition.string.begin.markdown, punctuation.definition.string.end.markdown, punctuation.definition.constant.markdown",
			"settings": {
				"foreground": "#8C8C8C"
			}
		},
		{
			"scope": "punctuation.definition.template-expression, punctuation.section.embedded",
			"settings": {
				"foreground": "#fa3e83"
			}
		},
		{
			"scope": "meta.template.expression",
			"settings": {
				"foreground": "#F8F8F2"
			}
		}
	],
	"semanticTokenColors": {
		"class": {
			"foreground": "#f8f8f2",
			"fontStyle": "underline"
		},
		"enum": {
			"foreground": "#66D9EF",
			"fontStyle": ""
		},
		"enumMember": {
			"foreground": "#f8f8f2",
			"fontStyle": "italic"
		},
		"property": {
			"foreground": "#c8c8c2",
			"fontStyle": "underline"
		},
		"variable": {
			"foreground": "#c8c8c2",
			"fontStyle": ""
		}
	  }
}
